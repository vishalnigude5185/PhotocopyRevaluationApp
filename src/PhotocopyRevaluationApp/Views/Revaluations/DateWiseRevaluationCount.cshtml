@model IEnumerable<PhotocopyRevaluationApp.Models.DatewiseRevaluationCountDTO>
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
    ViewData["X_Axis"] = "CreatedDate";
}

@await Html.PartialAsync("_TableData")

<h4>Date Wise Revaluation Count</h4>
<table border="1" align="center" id="myDataTable" class="display">
    <tr>
        <th><b>SNO</b></th>
        <th><b>Date</b></th>
        <th><b>Revaluation Count</b></th>
    </tr>
    @{
        int i = 1;
        foreach (var item in Model)
        {
            <tr>
                <td>@i</td>
                <td>@item.CreatedDate</td>
                <td><a href="@Url.Action("_List", new { ColumnName = "CreatedDate", Value = @item.CreatedDate.Date })">@item.RevaluationCount</a></td>
            </tr>
            i++;
        }
    }
</table>
